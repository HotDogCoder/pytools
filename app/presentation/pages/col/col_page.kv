<ColPage>:
    name: "COL"
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        Label:
            text: "Col Screen"
            font_size: 32

        Button:
            text: "Go to Ad"
            font_size: 32
            on_release:
                app.root.current = "AD"
                root.manager.transition.direction = "left"